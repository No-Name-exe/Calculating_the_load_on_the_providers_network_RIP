{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Python Debugger: Django",
			"type": "debugpy",
			"request": "launch",
			"program": "${workspaceFolder}\\lab1\\manage.py",
			"args": ["runserver", "8080"],
			"django": true,
			"justMyCode": true
		},
		{
			"name": "Run React (Vite)",
			"type": "node",
			"request": "launch",
			"runtimeExecutable": "npm",
			"runtimeArgs": [
				"run",
				"dev"
     		],
			"cwd": "${workspaceFolder}\\React\\my-app",
			"console": "integratedTerminal",
			"internalConsoleOptions": "neverOpen"
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "no CORS",
			"file": "${relativeFile}",
			"runtimeArgs": [
				"--disable-web-security"
			]
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Site",
			"file": "Site.html",
			"runtimeArgs": [
				"--disable-web-security"
			]
		},
	],
	"compounds": [
	]
}